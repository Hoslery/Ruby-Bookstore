<% form({ method: :post },{ obj: @parameters }) do |f| %>
  <div class="grid-x grid-margin-x">
    <div class="large-12 cell">
      <%= f.input(:file_name, type: :text, label: 'Название файла, куда запишутся все выбранные товары', placeholder: 'filename')%>
    </div>
    <div class="large-12 cell">
      <%= f.button(value: 'Оплатить', class: 'button') %>
      <a href="/shop/list" class="button secondary">Отмена</a>
    </div>
  </div>
<% end %>